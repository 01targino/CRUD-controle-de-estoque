package com.controleDeEstoque.ControleDeEstoque.usuarios;

import java.math.BigDecimal;
import java.time.LocalDate;

import com.controleDeEstoque.ControleDeEstoque.produto.Categoria;
import com.controleDeEstoque.ControleDeEstoque.produto.Fornecedor;
import com.controleDeEstoque.ControleDeEstoque.produto.Produto;

import jakarta.persistence.Entity;
import jakarta.persistence.Table;
import lombok.AllArgsConstructor;
import lombok.EqualsAndHashCode;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;

@Entity(name = "produtos")
@Table(name = "produtos")
@Getter
@Setter
@AllArgsConstructor
@NoArgsConstructor
@EqualsAndHashCode(of="id")
public class Usuario {

	private Long id;
	
	private String login;
	
	private String senha;
	
	
	
}
